{
  "name": "listmusik",
  "description": "A simple music organiser",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ed359/listmusik.git"
  },
  "main": "index.html",
  "window": {
    "toolbar": true,
    "width": 800,
    "min_width": 800,
    "height": 600
  },
  "platformOverrides": {
    "win": {
      "window": {
        "toolbar": false
      }
    },
    "osx": {
      "window": {
        "toolbar": false
      }
    }
  },
  "author": "Ewan Davies <ewan.davies@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "escape-string-regexp": "^1.0.4",
    "ffmetadata": "^1.4.0",
    "file-url": "^1.0.1",
    "fs-extra": "^0.30.0",
    "itunes-data": "^0.1.1",
    "jade": "^1.11.0",
    "lodash": "^4.2.1",
    "mkdirp": "^0.5.1",
    "musicmetadata": "^2.0.2",
    "nml-parser": "git+https://github.com/discreteinfinity/nml-parser.git",
    "semver": "^5.1.0"
  }
}
